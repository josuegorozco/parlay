{
  "name": "parlay",
  "version": "1.0.0",
  "description": "Boilerplate",
  "scripts": {
    "security-check": "nsp check",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "cross-env NODE_ENV=development babel-node server/index.js",
    "serve:dist": "npm-run-all --parallel serve:prod open:browser",
    "serve:prod": "cross-env NODE_ENV=production node dist/server.js",
    "open:browser": "/bin/sleep 1 && open http://localhost:3000",
    "lint": "esw build/webpack/webpack.config.dev.* app build --color --config \"./.eslintrc.js\"",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:watch": "npm run lint -- --watch",
    "lint:staged": "lint-staged",
    "test": "mocha --reporter progress build/tests/index.js \"app/**/*.test.js\" --compilers js:babel-register --require babel-polyfill",
    "test:spec": "npm run test -- --reporter spec",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node server/api/data",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch server/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test:spec lint",
    "build": "cross-env NODE_ENV=production babel-node build/index.js",
    "postbuild": "npm-run-all build:server serve:dist",
    "build:server": "babel server/production.js -o dist/server.js",
    "generate": "plop --plopfile build/generators/index.js",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josuegorozco/parlay.git"
  },
  "author": "Josue Orozco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josuegorozco/parlay/issues"
  },
  "homepage": "https://github.com/josuegorozco/parlay#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "bootstrap": "^4.0.0-alpha.5",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
