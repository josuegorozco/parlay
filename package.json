{
  "name": "parlay",
  "version": "1.0.0",
  "description": "Boilerplate",
  "scripts": {
    "security-check": "nsp check",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node server/index.js",
    "lint": "esw build/webpack/webpack.config.dev.* app build --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress build/tests/index.js \"app/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node server/api/data",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch server/api/db.json --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josuegorozco/parlay.git"
  },
  "author": "Josue Orozco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josuegorozco/parlay/issues"
  },
  "homepage": "https://github.com/josuegorozco/parlay#readme",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.6.0",
    "eslint-watch": "^2.1.14",
    "jsdom": "^9.8.3",
    "json-schema-faker": "^0.3.7",
    "json-server": "^0.9.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "npm-run-all": "^3.1.1",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
